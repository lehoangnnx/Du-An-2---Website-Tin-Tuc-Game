<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="defaultlayout"
		template="/WEB-INF/views/layout/default/layout-default.jsp">

		<put-attribute name="head"
			value="/WEB-INF/views/layout/default/head/head.jsp" />
		<put-attribute name="headtag"
			value="/WEB-INF/views/layout/default/headtag/headtag.jsp" />
		<put-attribute name="navbar"
			value="/WEB-INF/views/layout/default/navbar/navbar.jsp" />
		<put-attribute name="sitebar"
			value="/WEB-INF/views/layout/default/sitebar/sitebar.jsp" />

		<put-attribute name="body" value="" />
		<put-attribute name="footer"
			value="/WEB-INF/views/layout/default/footer/footer.jsp" />
		<put-attribute name="footertag"
			value="/WEB-INF/views/layout/default/footertag/footertag.jsp" />

		<put-attribute name="scriptfootertag"
					   value="" />
	</definition>


	<definition name="home" extends="defaultlayout">

		<put-attribute name="body" value="/WEB-INF/views/default/index.jsp" />
		<put-attribute name="scriptfootertag"
					   value="/WEB-INF/views/default/footertag/index-footertag.jsp" />

	</definition>

	<definition name="chitiet" extends="defaultlayout">
		<put-attribute name="scriptfootertag" value="/WEB-INF/views/default/footertag/chitiet-footertag.jsp" />
		<put-attribute name="body"
			value="/WEB-INF/views/default/chitietbaiviet.jsp" />
	</definition>

	<definition name="tonghop" extends="defaultlayout">
		<put-attribute name="body"
					   value="/WEB-INF/views/default/trangtonghop.jsp" />

	</definition>
	<definition name="hosogame" extends="defaultlayout">
		<put-attribute name="body"
					   value="/WEB-INF/views/default/tranggame.jsp" />

	</definition>
	<definition name="403" extends="defaultlayout">
		<put-attribute name="title" value="Trang Loi" />
		<put-attribute name="body" value="/WEB-INF/views/default/403.jsp" />
		<put-attribute name="sitebar" value="" />

	</definition>

	<definition name="forgotpassword" extends="defaultlayout">
		<put-attribute name="title" value="Lấy Lại Mật Khẩu" />
		<put-attribute name="body" value="/WEB-INF/views/default/forgotpassword.jsp" />
		<put-attribute name="sitebar" value="" />
	</definition>



</tiles-definitions>